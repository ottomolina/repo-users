package com.gsa.ctrl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.jdbc.core.JdbcTemplate;

import com.gsa.data.RequestData;
import com.gsa.data.ResponseData;
import com.gsa.data.ResponseData.Error;
import com.gsa.util.Util;


public class TablaServiceCtrl {
	private ResponseData responseData;
	@Autowired
	private JdbcTemplate jdbc;
	
	public ResponseData saveTabla(RequestData input) {
		responseData = new ResponseData();
		List<Error> listError = Util.verificaRequest(input);
		if(listError.size() != 0) {
			responseData.setResult("ERROR");
			responseData.setListError(listError);
			return responseData;
		}
		
		
		
		return null;
	}
	
}
