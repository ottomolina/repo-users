package com.gsa.interfaces;

import java.sql.Connection;
import java.sql.SQLException;
import java.util.List;

public interface IDao {
	
	public Integer getNextId(Connection conn) throws SQLException;
	
	public boolean isDuplicate(Connection conn, Object orm) throws SQLException;
	
	public Integer insert(Connection conn, Object orm) throws SQLException;
	
	public <T> List<T> select(Connection conn, T orm) throws SQLException;
	
	public Integer update(Connection conn, Object orm) throws SQLException;
	
	public Integer delete(Connection conn, Object orm) throws SQLException;
	
}
